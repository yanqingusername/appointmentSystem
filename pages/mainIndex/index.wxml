      <view style="margin: 30rpx 30rpx 0rpx 30rpx;">
         <view class="notice_view1" >
        <swiper class="swiper_zi" vertical="true" autoplay="true" circular="true" interval="3000">
            <block wx:for="{{3}}" wx:key="index">
            <swiper-item>
                <view class="notice_left" bind:tap="bindNoticeClick" data-index="{{index}}">
                  <image class="notice_left_img" src="../../images/icon_0701_11.png"></image>
                  <view class="notice_left_title">您的预约已进入<text class="notice_left_text">“待采样”</text>状态，点击查看详情{{index}}</view>
                </view>
            </swiper-item>
            </block>
        </swiper>
         </view>
      </view>

      <view class="main_bottom">常用服务</view>

      <view class="main_bottom_view">
        <view class="bottom_view_1" bind:tap="bindTestReport">
          <image class="bottom_view_1_img" src="../../images/icon_0701_12.png"></image>
          <view class="bottom_view_1_text">查询报告</view>

          <image class="main_15" src="../../images/icon_0701_15.png"></image>
        </view>
        <view class="bottom_view_1" bind:tap="bindEpidemicPreventionPolicy">
          <image class="bottom_view_1_img" src="../../images/icon_0701_13.png"></image>
          <view class="bottom_view_1_text">防疫政策</view>
        </view>
        <view class="bottom_view_1" bind:tap="bindXinguanSelfInspection">
          <image class="bottom_view_1_img" src="../../images/icon_0701_14.png"></image>
          <view class="bottom_view_1_text">新冠自检</view>
        </view>
      </view>

      <view class="empty_view"></view>