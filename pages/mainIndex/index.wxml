  <view style="position:relative;">
    <image src="../../images/icon_0701_05.png" style="height:302px;width:100%"/>
    <view class="main_view1">
      <view class="main_title">卡尤迪新冠肺炎核酸检测</view>
      <view class="main_title_1"><view class="main_radius_1"></view><view class="main_text">12小时内出报告</view></view>
      <view class="main_title_2" bindtap="bindChooseMap"><view class="main_radius_2"></view><view class="main_text">91个核酸检测采样点位，看地图 ></view></view>
    </view>
    <image src="../../images/icon_0701_06.png" class="main_img" />
  </view>
<view class="main_center">
  <view class="main_center_left" bindtap="bindOnsite" data-type="0">
    <view class="main_1">现场录信息</view>
    <view class="main_2">登记个人信息，排队采样</view>
    <view class="main_3">马上预约</view>
    <image src="../../images/icon_0701_07.png" class="main_7" />
    <image src="../../images/icon_0701_08.png" class="main_8" />
  </view>

  <view class="main_center_right">
    <view class="main_6" bindtap="bindChooseMap">
      <view class="main_1">搜索采样点</view>
      <view class="main_2">社区，商圈，火车站</view>
      <image src="../../images/icon_0701_09.png" class="main_9" />
    </view>

    <view class="main_11" bindtap="bindAppointment">
      <view class="main_1">预约记录</view>
      <view class="main_2">我的信息码，样本进度</view>
      <image src="../../images/icon_0701_10.png" class="main_10" />
    </view>
  </view>


</view>

