          <view class="map_3_view_title" wx:else><image class="icon_1_new" style="margin-right:10rpx;" src="../../images/icon_position_06.png"></image>{{item.screen_text}}<view style="color: #FF9900;font-size: 22rpx;padding-top:4rpx;margin-left:10rpx;">{{item.big_screen_text}}</view></view>
