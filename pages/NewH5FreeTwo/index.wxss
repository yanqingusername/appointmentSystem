page{
  background:#F6F6F6;
}

.page{
  display: flex;
  /* align-items: center; */
  justify-content: center;
  flex-direction: column;
  margin: 30rpx 30rpx 30rpx 30rpx;
}

.medWhite{
  background-color: #fff;
  border-radius: 20rpx;
  /* box-shadow: 0px 4px 16px 0px rgba(0,0,0,0.06); */
}
.sampleSite{
  display: flex;
  justify-content: center;
  /* align-items: center; */
  flex-direction: column;
  padding: 30rpx;
}

.sampleSite_1{
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
}

.sampleSite_2{
  padding-top: 20rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #999999;
}

.subject_view {
  /* display: flex;
    align-items: center;
    justify-content: center; */
  /* width: 90%; */
  background: #FFFFFF;
  border-radius: 10px;
  margin: 30rpx 0rpx 0rpx 0rpx;
  /* left: 5%; */
  /* right: 5%; */
  position: relative;
}


.subject_view_red {
  width: 100%;
  height: 100rpx;
  background: #E26396;
  border-radius: 10px;
}

.subject_view_white {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  background: #FFFFFF;
  border-radius: 10px;
  /* padding: 28rpx 0rpx; */
  font-size: 32rpx;
  font-weight: 500;
  color: #E06596;
  position: absolute;
  top: 6rpx;
  height: 100rpx;
}
.icon_add{
  width: 40rpx;
  height: 40rpx;
}

.subject_view_bg {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  background: #FFFFFF;
  border-radius: 10px;
  padding: 28rpx 0rpx 28rpx 30rpx;
  font-size: 32rpx;
  font-weight: 500;
  border-top: 10rpx solid #E26396;
  /* position: absolute;
  top: 6rpx;
  height: 230rpx; */
}

.subject_title{
  font-size: 40rpx;
font-weight: 500;
color: #333333;
flex-wrap: wrap;
    overflow-wrap: anywhere;
    word-break: break-all;
}

.subject_text{
  font-size: 32rpx;
font-weight: 400;
color: #999999;
}

.right_arrow{
  height: 50rpx;
  width: 60rpx;
}

.fixed_view{
  position: fixed;
    bottom: 0rpx;
    z-index: 1001;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    /* height: 150rpx; */
    background: #FFFFFF;
    padding: 30rpx 30rpx;
}
.view_3{
  display: flex;
  align-items: center;
  justify-content: center;
height: 100rpx;
width: 100%;
background: #E06596;
border-radius: 24px;
font-size: 34rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #FFFFFF;
}


.view_1{
  background-color: #fff;
  border-radius: 20rpx;
  margin-top: 30rpx;
  /* box-shadow: 0px 4px 16px 0px rgba(0,0,0,0.06); */
}
.view_2{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
}

.view_5{
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
}

.view_6{
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #999999;
}

